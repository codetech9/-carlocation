<div class="container">
  <%= simple_form_for(@car) do |f| %>
    <%= f.input :brand %>
    <%= f.input :model %>
    <%= f.input  :km %>
    <%= f.input  :year %>
    <%= f.input :options %>
    <%= f.input  :color %>
    <%= f.input  :price %>
     <div  class="mx-auto">
      <%= f.button :submit, class: "btn btn-primary" %>
    </div>
  <% end %>
</div>
